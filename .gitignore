# Ignore bundler config
/.bundle/

# Ignore all logfiles and tempfiles.
/log/
/tmp/
/pidfile
/sessions/
/sockets/
/coverage/
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore Byebug command history file.
.byebug_history

# Ignore macOS system files
.DS_Store

# Ignore Ruby specific files
*.gem
*.rbc
.ruby-version
.ruby-gemset

# Ignore Jekyll output folder
_site/

# Ignore Gemfile.lock, as it can be generated by running `bundle install`
Gemfile.lock

# Ignore hidden files
.sass-cache/
.jekyll-cache/
jekyll-cache/

# Ignore the generated assets
assets/css/*.css
assets/css/*.css.map
assets/js/*.js

# Ignore coverage files generated by test suites
/coverage/

# Ignore any locally installed gems
vendor/bundle

# Ignore any downloaded dependencies
.bundle/

# Ignore compiled files
*.o
*.so
*.idb
*.pid
*.rej

# Ignore IDE or Editor folders
.vscode/
.idea/

# Ignore Node.js modules (if used)
/node_modules/

# Ignore backup files
*.swp
*~

# Ignore the videos directory
assets/videos/

# Ignore any potential secret keys
.env

# Ignore any potentially large binary files (e.g., recorded sessions)
*.mov

# Ignore thumbnail images
/thumbnail.png
